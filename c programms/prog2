	printf("Dose 2o arithmo: \n");
	scanf("%lf",&b);

	pro=a+b;
	af=a-b;
	pol=a*b;
	di=a/b;

	printf("To apotelesma tis prosthesis :%lf\n",pro);
	printf("To apotelesma tis aferesis :%lf\n",af);
	printf("To apotelesma toy polaplasiasmoy :%lf\n",pol);
