#include <stdio.h>
#include <stdlib.h>

int main()
{
    double a, b;
	int i;


	for ( i=0; i<100; i++ ) {


	printf("Dose proto arithmo: \n");
	scanf("%lf",&a);


	printf("Dose deytero arithmo: \n");
	scanf("%lf",&b);

	if( a>b )
        {  printf(" O a megaliteros\n");	}

    else if (b>a)
        {  printf (" O b megaliteros\n"); }

    else

		{ printf("O a kai o b einai isoi\n"); }



	printf("***************************\n");



}
	return 0;
}
