#include <stdio.h>
#include <stdlib.h>

int main()
{

     int i=1;

     int sum=0;

     double mo;

    while (i<=10000)
    {
       sum=sum+i;

        i++;
    }


        mo=sum/10000;

        printf ("Sinoliko athroisma: %d\n", sum);
        printf ("O mesos oros einai: %lf\n", mo);


    return 0;
}
