#include <stdio.h>
#include <stdlib.h>

int main()
{
    int A[10];
    double mo;
    int sum=0;

    for (int i=0; i<=10; i++)
    {
        printf("Dose stoixia : \n");
        scanf("%d",&A[i]);

    }

    for (int i=0; i<=10; i++)
    {
        sum=sum+A[i];
    }


    mo=sum/10;

    printf("O mesos oros einai : %lf\n",mo);


    return 0;
}
