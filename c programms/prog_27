#include <stdio.h>
#include <stdlib.h>

int main()
{
    int A[10], B[10], C[10], D[10], E[10], F[10], sum1=0 ,sum2=0 ;
    double mo1,mo2;


     for (int i = 1; i<=10; i++)
      {

          printf("Dose times ston pinaka A sti thesi: %i \n", i);
          scanf("%d", &A[i]);
          printf("*****************************************\n");
        }


        printf("*****************************************\n");
        printf("*****************************************\n");
        printf("*****************************************\n");


      for (int i = 1; i<=10; i++)
      {
          printf("Dose times ston pinaka B sti thesi: %i \n", i);
          scanf("%d", &B[i]);
          printf("*****************************************\n");
        }

    for (int i = 1; i<=10; i++)
    {
        C[i] = A[i]+B[i];
    }

    printf("*****************************************\n");
    printf("*****************************************\n");
    printf("*****************************************\n");

    for (int i = 1; i<=10; i++)
    {
        printf("Oi times tou athristikou pinaka C ana thesi: %d \n", C[i]);
    }


    printf("*****************************************\n");
    printf("*****************************************\n");
    printf("*****************************************\n");

    for (int i=1; i<=10; i++)
    {
        D[i]=A[i]-B[i];

    }




    for (int i=1;  i<=10; i++)
    {
        printf("Oi times tou pinaka diaforon D ana thesi : %d\n",D[i]);
    }

    printf("*****************************************\n");
    printf("*****************************************\n");
    printf("*****************************************\n");


    for(int i=1; i<=10; i++)
    {
        E[i]=A[i]*B[i];
    }

    for(int i=1; i<=10; i++)
    {
        printf("oi times toy pinaka ginomenon E ana thesi : %d\n",E[i]);

    }

    printf("*****************************************\n");
    printf("*****************************************\n");
    printf("*****************************************\n");

    for (int i=1; i<=10; i++)
    {
        F[i]=A[i]/B[i];
    }

    for (int i=1; i<=10; i++)
    {
        printf("Oi times toy pinaka pilikon F ana thesi : %d\n",F[i]);
    }

    printf("*****************************************\n");
    printf("*****************************************\n");
    printf("*****************************************\n");


    for (int i=1; i<=10; i++)
    {
        sum1=sum1+A[i];
    }

            mo1=sum1/10;
            printf("O mesos oros tou pinaka A einai: %lf\n",mo1);

    printf("*****************************************\n");
    printf("*****************************************\n");
    printf("*****************************************\n");


    for (int i=1; i<=10; i++)
    {
        sum2=sum2+B[i];
    }
        mo2=sum2/10;
        printf("o mesos oros toy pinaka B einai : %lf\n",mo2);

    printf("*****************************************\n");
    printf("*****************************************\n");
    printf("*****************************************\n");


    return 0;
}
