#include <stdio.h>
#include <stdlib.h>

int main()
{
    double a, b;
	int i;


	for (i=0; i<100; i++){


	printf("Dose ton proto arithmo : \n");
	scanf("%lf",&a);
	printf("Dose ton deytero arithmo : \n");
	scanf("%lf",&b);

